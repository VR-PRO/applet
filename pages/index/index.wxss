.home-bg{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #11091f;
  background-image: url("https://wt-box.worktile.com/thumb/dfdf76db-b83d-4e64-8bd1-e6ad85650161_480x360.png");
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.home-img{
  display: block;
  margin: 200rpx auto 50rpx;
  width: 329rpx;
  height: 316rpx;
}
.home-tit{
  font-size: 38rpx;
  color: #fff;
  margin-bottom: 22rpx;
  text-align: center
}
.home-prompt{
  font-size: 28rpx;
  line-height: 43rpx;
  color: #cdcdcd;
  text-align: center;
}
.home-btn{
  width: 366rpx;
  height: 80rpx;
  margin-top: 53rpx;
  font-size: 32rpx;
  color: #666;
}
.home-btn-icon{
  float: left;
